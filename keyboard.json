{
  "rules": [
    {
      "description": "Use Hyper + D to F13",
      "manipulators": [
        {
          "from": {
            "key_code": "d",
            "modifiers": {
              "mandatory": ["left_control", "left_option", "left_shift"]
            }
          },
          "to": [
            {
              "key_code": "f13"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Use Hyper + 5 to Caps Lock",
      "manipulators": [
        {
          "from": {
            "key_code": "5",
            "modifiers": {
              "mandatory": ["left_control", "left_option", "left_shift"]
            }
          },
          "to": [
            {
              "key_code": "caps_lock"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Use fn(globe) to control + command + space",
      "manipulators": [
        {
          "from": {
            "key_code": "fn"
          },
          "to": [
            {
              "key_code": "spacebar",
              "modifiers": ["left_command", "left_control"]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Use Hyper + E to control + up_arrow",
      "manipulators": [
        {
          "from": {
            "key_code": "e",
            "modifiers": {
              "mandatory": ["left_control", "left_option", "left_shift"]
            }
          },
          "to": [
            {
              "key_code": "up_arrow",
              "modifiers": ["left_control"]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Use Hyper + \\ to Fn + C (Control Center)",
      "manipulators": [
        {
          "from": {
            "key_code": "backslash",
            "modifiers": {
              "mandatory": ["left_control", "left_option", "left_shift"]
            }
          },
          "to": [
            {
              "key_code": "c",
              "modifiers": ["fn"]
            }
          ],
          "type": "basic"
        }
      ]
    }
  ],
  "title": "Hyper Key Combinations"
}
